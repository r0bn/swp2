.container-fluid
    .row
        .col-md-12
                ul.nav.nav-tabs(id="myTabs")
                    li.active(id="GraEditorTab")
                        a(ng-click="tabSwitch('GraEditorTab')", data-toggle="tab") Graphischer Editor
                    li(id="MedienBibTab")
                        a(ng-click="tabSwitch('MedienBibTab')", data-toggle="tab") Medienbibliothek

                .tab-content(id="myTabContent")
                    .tab-pane.fade.active.in(id="GraEditor") 
                        include ./formular.jade

                    .tab-pane.fade.active.in(id="MedienEditor", style="display:none")
                        include ./library.jade 
